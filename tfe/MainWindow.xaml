<Window x:Class="tfe.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:tfe"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="1129.455">
    <Grid>
        <GroupBox Header="importer un fichier" HorizontalAlignment="Left" Height="153" Margin="10,257,0,0" VerticalAlignment="Top" Width="397">
            <Grid HorizontalAlignment="Left" Height="117" Margin="4,10,-3,0" VerticalAlignment="Top" Width="383">
                <Button Content="importer" Click="TestBtn_Click" x:Name="TestBtn" HorizontalAlignment="Left" Margin="298,88,0,0" VerticalAlignment="Top" Width="75"/>
                <TextBox Name="Impfile" HorizontalAlignment="Left" Height="19" Margin="129,88,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="169"/>
            </Grid>
        </GroupBox>
        <GroupBox Header="Enregistrer" HorizontalAlignment="Left" Height="237" Margin="10,15,0,0" VerticalAlignment="Top" Width="198">
            <Grid HorizontalAlignment="Left" Height="215" Margin="-5,4,-4.4,-4.6" VerticalAlignment="Top" Width="195">
                <Button Content="record" Click="StartBtn_Click" x:Name="StartBtn" IsEnabled="True"  HorizontalAlignment="Left" Margin="10,38,0,0" VerticalAlignment="Top" Width="75"/>
                <Button Content="stop" Click="StopBtn_Click" x:Name="StopBtn" IsEnabled="False"  HorizontalAlignment="Left" Margin="10,84,0,0" VerticalAlignment="Top" Width="75"/>
            </Grid>
        </GroupBox>
        <TextBlock Name="frequence" HorizontalAlignment="Left" Margin="412,10,0,0" TextWrapping="Wrap" Text="TextBlock" VerticalAlignment="Top" Height="400" Width="182"/>
        <TextBlock Name="notes" HorizontalAlignment="Left" Margin="718,10,0,0" TextWrapping="Wrap" Text="TextBlock" VerticalAlignment="Top" Height="400" Width="345"/>

    </Grid>
</Window>
